{
    "parser": "@typescript-eslint/parser",
    "rules": {
        "no-unused-vars": "warn"
    },
    "extends": [
        "plugin:react/recommended",
        "plugin:@typescript-eslint/recommended"
    ],
    "parserOptions": {
        "ecmaVersion":  2018,
        "sourceType":  "module",
        "ecmaFeatures":  {
            "jsx":  true
        }
    },
    "settings":  {
        "react":  {
            "version":  "detect"
        }
    }
}